SELECT g.oii_3726_flux, g.oii_3726_flux_err, g.oii_3729_flux, g.oii_3729_flux_err, g.neiii_3869_flux, g.neiii_3869_flux_err, g.h_delta_flux, g.h_delta_flux_err, g.h_gamma_flux, g.h_gamma_flux_err, g.oiii_4363_flux, g.oiii_4363_flux_err, g.h_beta_flux, g.h_beta_flux_err, g.oiii_4959_flux, g.oiii_4959_flux_err, g.oiii_5007_flux,g.oiii_5007_flux_err,g.hei_5876_flux, g.hei_5876_flux_err, g.oi_6300_flux, g.oi_6300_flux_err, g.nii_6548_flux, g.nii_6548_flux_err, g.h_alpha_flux, g.h_alpha_flux_err,g.nii_6584_flux, g.nii_6584_flux_err, g.sii_6717_flux, g.sii_6717_flux_err, g.sii_6731_flux, g.sii_6731_flux_err, g.ariii7135_flux, g.ariii7135_flux_err, g.oii_flux, g.oii_flux_err, g.oiii_flux, g.oiii_flux_err, e.Flux_HeII_4685, e.Flux_HeII_4685_Err, e.Flux_OII_3726, e.Flux_OII_3726_Err
FROM #upload u
      JOIN #x x ON x.up_id = u.up_id
      JOIN SpecObjAll s ON s.specObjID = x.specObjID
      JOIN GalSpecLine g ON s.specobjid = g.specobjid
      JOIN emissionLinesPort e ON s.specobjid = e.specobjid
WHERE
    h_alpha_flux > h_alpha_flux_err*5
    AND h_beta_flux > h_beta_flux_err*5
    AND oiii_5007_flux > oiii_5007_flux_err*5
    AND nii_6548_flux > nii_6548_flux_err*5
    AND nii_6584_flux > g.nii_6584_flux_err*5
    AND e.Flux_HeII_4685 > e.Flux_HeII_4685_Err*5
